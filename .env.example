# AI API Keys
OPENAI_API_KEY=your_openai_api_key_here  # Required for Swarms agents
SWARMS_API_KEY=your_swarms_api_key_here  # Optional: Direct Swarms API usage
SWARMS_API_BASE=https://api.swarms.world  # Optional: Swarms API endpoint

# Browser Configuration
HEADLESS=True  # Set to False for visible browser
PROXY_URL=your_proxy_url_here  # Required for geo-restriction bypass
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Target Platform
BASE_URL=https://pump.fun  # Main platform URL
TOKEN_URL_PATTERN={base_url}/coin/{token_address}  # Token page pattern

# Storage Configuration
SCREENSHOT_DIR=analysis_screenshots  # Directory for chart screenshots
ANALYSIS_DIR=analysis_results  # Directory for analysis results

# Agent Configuration
MODEL_NAME=gpt-4  # Model for Swarms agents
SWARMS_WORKFLOW=sequential  # Workflow type: sequential or parallel

# Performance Settings
MAX_CONCURRENT_AGENTS=10  # Maximum number of parallel agents
ANALYSIS_INTERVAL=5  # Seconds between analyses
CHART_CONFIDENCE_THRESHOLD=0.85  # Minimum confidence for pattern detection
RISK_THRESHOLD=0.30  # Maximum acceptable risk score
